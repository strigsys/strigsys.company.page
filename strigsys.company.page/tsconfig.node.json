{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",

    "lib": [ "DOM", "DOM.Iterable", "ESNext" ],

    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,

    "jsx": "react-jsx",

    "types": [ "node", "react", "react-dom" ]
  },

  "include": [ "vite.config.ts" ]
}
